term User {
    id : Long
    firstName : String
    lastName : String
    username : String
    password : String
    active : boolean
}

term Firm {
    id : Long
    name : String
    email : String
    workersNumber : int
    description : String
    address : Address
}

term Address {
    id : Long
    street : String
    number : int
    city : String
    country : String
}